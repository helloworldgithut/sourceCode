<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sendi.dao.ProductDaoI">
    <select id="productList" parameterType="java.lang.Integer"
        resultType="com.sendi.entity.Product">
        select * from t_product where id = #{id}
    </select>

    <select id="queryByProId" parameterType="java.lang.Integer"
            resultType="com.sendi.entity.Product">
        select * from t_product where id = #{id}
    </select>

</mapper>